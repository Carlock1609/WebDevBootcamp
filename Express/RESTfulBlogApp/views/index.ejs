<%- include('partials/header') %>


<% blogs.forEach(function(blog) { %>
	<div>
		<h4>
			<%= blog.created %>
		</h4>
		<h1>
			<%= blog.title %>
		</h1>
		<img src='<%= blog.image %>'>
		<p>
			<%= blog.body.substring(0,100)%>...
		<p>		 
		<a href='/blogs/<%= blog._id %>'>Read More</a>
	</div>	
<% }) %>


<%- include('partials/footer') %>